{
  "name": "one-wallet",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "compile": "tsc --noEmit -p . --pretty",
    "lint:eslint": "eslint app test e2e --ext .js,.ts,.tsx",
    "lint:eslint:fix": "eslint app test e2e --ext .js,.ts,.tsx --fix",
    "lint:prettier": "prettier --check \"app/**/*.{js,json,ts{,x},md}\"",
    "lint:prettier:fix": "prettier --write \"app/**/*.{js,json,ts{,x},md}\"",
    "lint:ts": "tsc --noEmit",
    "lint": "yarn lint:prettier && yarn lint:eslint && yarn lint:ts",
    "lint:fix": "yarn lint:prettier:fix && yarn lint:eslint:fix",
    "patch": "patch-package",
    "test": "jest",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "build-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "clean": "react-native-clean-project",
    "clean-all": "npx react-native clean-project-auto",
    "translations:sort": "node ./.scripts/sort-translations.js",
    "rnuc:dev": "yarn rnuc ./app/config/flavors/procivis/dev/.env",
    "rnuc:test": "yarn rnuc ./app/config/flavors/procivis/test/.env",
    "rnuc:trial": "yarn rnuc ./app/config/flavors/procivis/trial/.env",
    "rnuc:demo": "yarn rnuc ./app/config/flavors/procivis/demo/.env",
    "check:licenses": "npx license-checker-rseidelsohn --production --direct --csv --out licenses.csv",
    "check:licenses:full": "npx license-checker-rseidelsohn --production --csv --out licenses_full.csv"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.13.0",
