  },
  "engines": {
    "node": ">=18.18"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/test/setup.ts",
      "<rootDir>/node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@procivis|@react-native|@react-native-async-storage|@react-navigation|@react-native-community|rn-material-ui-textfiel|react-native-screens|react-native-reanimated|expo-)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mock-file.ts",
      "\\.svg": "<rootDir>/test/mock-svg.ts"
    }
  },
  "packageManager": "pnpm@10.5.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@procivis/react-native-one-core",
      "@sentry/cli",
      "canvas",
      "detox",
      "dtrace-provider",
      "ejs",
      "sharp"
    ]
  }
